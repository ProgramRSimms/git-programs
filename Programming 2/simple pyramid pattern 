#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc, char* argv[]){
    int i, N, row;
    scanf("%d", &N);

    for (row = 0; row < N; row++){
        for (i = 0; i < N; i++){
            printf(" ");
        }
    }
    for (i = 0; i < 2 * row; i++){
        printf("*");
    }
    printf("\n");


   return 0;
}